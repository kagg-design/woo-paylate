{
  "name": "kagg-design/woo-paylate",
  "description": "Gateway for PayLate on WooCommerce to provide payments via PayLate service.",
  "license": "GPL-2.0",
  "keywords": [
    "woocommerce",
    "paylate",
    "payment"
  ],
  "homepage": "https://kagg.eu/en/",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "support": {
    "issues": "https://wordpress.org/support/plugin/woo-paylate"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.0"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.9.0",
    "phpcompatibility/php-compatibility": "^9.3.5",
    "phpcompatibility/phpcompatibility-wp": "^2.1.4",
    "wp-coding-standards/wpcs": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "KAGG\\Paylate\\": "src/php"
    }
  }
}
